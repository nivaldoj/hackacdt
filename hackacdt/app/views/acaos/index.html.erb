<p id="notice"><%= notice %></p>

<h1>Acaos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Descricao</th>
      <th>Dt inicio</th>
      <th>Dt fim</th>
      <th>Organizacao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @acaos.each do |acao| %>
      <tr>
        <td><%= acao.nome %></td>
        <td><%= acao.descricao %></td>
        <td><%= acao.dt_inicio %></td>
        <td><%= acao.dt_fim %></td>
        <td><%= acao.organizacao %></td>
        <td><%= link_to 'Show', acao %></td>
        <td><%= link_to 'Edit', edit_acao_path(acao) %></td>
        <td><%= link_to 'Destroy', acao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Acao', new_acao_path %>
