<p id="notice"><%= notice %></p>

<h1>Voluntarios</h1>

<table>
  <thead>
    <tr>
      <th>Nome completo</th>
      <th>Data nasc</th>
      <th>Profissao</th>
      <th>Email</th>
      <th>Telefone</th>
      <th>Endereco</th>
      <th>Cidade</th>
      <th>Cep</th>
      <th>Estado</th>
      <th>Empresa trabalha</th>
      <th>Voluntario</th>
      <th>Area atuacao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voluntarios.each do |voluntario| %>
      <tr>
        <td><%= voluntario.nome_completo %></td>
        <td><%= voluntario.data_nasc %></td>
        <td><%= voluntario.profissao %></td>
        <td><%= voluntario.email %></td>
        <td><%= voluntario.telefone %></td>
        <td><%= voluntario.endereco %></td>
        <td><%= voluntario.cidade %></td>
        <td><%= voluntario.cep %></td>
        <td><%= voluntario.estado %></td>
        <td><%= voluntario.empresa_trabalha %></td>
        <td><%= voluntario.voluntario %></td>
        <td><%= voluntario.area_atuacao %></td>
        <td><%= link_to 'Show', voluntario %></td>
        <td><%= link_to 'Edit', edit_voluntario_path(voluntario) %></td>
        <td><%= link_to 'Destroy', voluntario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voluntario', new_voluntario_path %>
