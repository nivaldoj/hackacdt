<div class="container">
<p id="notice"><%= notice %></p>

<h2>Parceiros</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Endereco</th>
      <th>Cep</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parceiros.each do |parceiro| %>
      <tr>
        <td><%= parceiro.nome %></td>
        <td><%= parceiro.endereco %></td>
        <td><%= parceiro.cep %></td>
        <td><%= parceiro.cidade %></td>
        <td><%= parceiro.estado %></td>
        <td><%= link_to 'Ver', parceiro %></td>
        <td><%= link_to 'Editar', edit_parceiro_path(parceiro) %></td>
        <td><%= link_to 'Excluir', parceiro, method: :delete, data: { confirm: 'VocÃª tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Parceiro', new_parceiro_path, :class=>"btn btn-default" %>

</div>
