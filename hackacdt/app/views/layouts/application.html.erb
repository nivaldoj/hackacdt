<!DOCTYPE html>
<html>
  <head>
    <title>Hackacdt</title>
    <%= csrf_meta_tags %>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
   <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
   <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
  </head>

  <body>

    <nav class="navbar navbar-default">
  <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header ">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a href="#"><%= image_tag("logo.png") %></a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
        <li><a href="#">Home <span class="sr-only">(current)</span></a></li>
        <li><a class="menus" href="#">Link</a></li>
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Cadastro <span class="caret"></span></a>
          <ul class="dropdown-menu">
            <li><a href="/parceiros">Parceiros</a></li>
            <li><a href="/voluntarios">Voluntarios</a></li>
            <li><a href="/organizacaos">Organizações</a></li>
            <li><a href="/acaos">Ações</a></li>
            <li role="separator" class="divider"></li>

          </ul>
        </li>
      </ul>

      <ul class="nav navbar-nav navbar-right">

        <li class="dropdown link-topo">
          <% if user_signed_in? %>
          <a href="#" class="dropdown-toggle link-topo" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><%= current_user.email %><span class="caret"></span></a>
          <ul class="dropdown-menu link-topo">
            <li role="separator" class="divider"></li>
            <li><%= link_to "sair", destroy_user_session_path, method: :delete%></li>
          </ul>
        </li>
          <% else %>
              <li class="dropdown link-topo">

                    <a href="#" class="dropdown-toggle link-topo" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Usuario não Logado<span class="caret"></span></a>
                    <ul class="dropdown-menu link-topo">
                      <li role="separator" class="divider"></li>
                      <li><%= link_to "Logar", new_user_session_path%></li>
                    </ul>
              </li>
       </ul>
       <% end %>
    </div>
  </div>
</nav>



    <%= yield %>




    <%= javascript_include_tag "https://code.jquery.com/jquery-3.1.1.min.js", "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.1/jquery-ui.min.js", "application" %>
      <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
      <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
      <script type="text/javascript" src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-price-format/2.1/jquery.priceformat.min.js"></script>
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.3/jquery.mask.min.js"></script>
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/canvasjs/1.7.0/jquery.canvasjs.min.js"></script>
      <script src="https://code.highcharts.com/highcharts.js"></script>
      <script src="https://code.highcharts.com/modules/exporting.js"></script>


      <%= javascript_include_tag 'custom', 'data-turbolinks-track' => true %>

  </body>
</html>
